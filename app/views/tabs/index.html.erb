<p style="color: green"><%= notice %></p>

<hr>
<%= link_to "New tab", new_tab_path %>
<hr>

<div id="tabs">
  <% current_user.tabs.each do |tab| %>
    <%#= render tab %>
    <p>
      <strong>Title:</strong>
      <%= tab.title %>
    </p>
    <p>
      <%= link_to "Show this tab", tab %>
    </p>
    <hr>
  <% end %>
</div>
